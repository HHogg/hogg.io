{
  "name": "hogg.io",
  "version": "0.0.0",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf public && parcel build src/index.html -d public && react-snap",
    "deploy": "firebase deploy",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.ts src/**/*.tsx",
    "lint": "yarn lint:js && yarn lint:css",
    "serve": "superstatic ./public --port 4000 -c superstatic.json",
    "start": "rm -rf dist && parcel src/index.dev.html --port 4000 -d dist --no-cache --no-autoinstall"
  },
  "dependencies": {
    "@bitrise/bitviz": "^0.0.9",
    "@hhogg/snake": "^0.0.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^8.2.3",
    "bezier-easing": "^2.0.3",
    "caniuse-lite": "^1.0.30000697",
    "classnames": "^2.2.5",
    "firebase-tools": "^7.14.0",
    "framer": "^1.1.7",
    "framer-motion": "^1.8.4",
    "gl-matrix": "^3.0.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "lodash.flatten": "^4.4.0",
    "lodash.groupby": "^4.6.0",
    "lodash.random": "^3.2.0",
    "luxon": "^1.22.0",
    "numbro": "^2.2.0",
    "open-color": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "postcss-preset-env": "^6.7.0",
    "preshape": "^4.5.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "6.0.0-beta.2 ",
    "react-router-dom": "^5.1.2",
    "react-snap": "^1.23.0",
    "regl": "^1.3.11",
    "sat": "^0.8.0",
    "typescript": "^3.7.5",
    "vishull2d": "^0.1.0"
  },
  "devDependencies": {
    "@types/bezier-easing": "*",
    "@types/classnames": "*",
    "@types/gl-matrix": "*",
    "@types/lodash.flatten": "*",
    "@types/lodash.groupby": "*",
    "@types/lodash.random": "*",
    "@types/luxon": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-helmet": "*",
    "@types/react-router-dom": "*",
    "@types/sat": "*",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-preshape": "^1.1.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "stylelint": "^13.2.0",
    "stylelint-config-preshape": "^2.0.0",
    "superstatic": "^6.0.4"
  },
  "reactSnap": {
    "source": "public",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  }
}
