{
  "name": "hogg.io",
  "version": "0.0.0",
  "author": "Harry Hogg <harry@hogg.io>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "build": "yarn run clean && yarn run build:client && yarn run build:server && yarn run ssg",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "clean": "rm -rf dist",
    "deploy": "firebase deploy",
    "dev": "vite --host localhost --port 4001",
    "format": "prettier --write .",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint \"src/**/*.tsx\" \"src/**/*.ts\" --fix",
    "lint": "yarn lint:css & yarn lint:js",
    "ssg": "node prerender",
    "ts:check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.3",
    "bezier-easing": "^2.1.0",
    "bitset": "^5.1.1",
    "classnames": "^2.2.5",
    "d3-scale": "^3.3.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.12.10",
    "gl-matrix": "^3.0.0",
    "lodash.flatten": "^4.4.0",
    "lodash.random": "^3.2.0",
    "lucide-react": "^0.291.0",
    "luxon": "^1.22.0",
    "numbro": "^2.2.0",
    "open-color": "^1.7.0",
    "preshape": "^16.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.1",
    "react-router-dom": "^6.11.1",
    "regl": "^2.0.1",
    "sat": "^0.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bezier-easing": "*",
    "@types/classnames": "*",
    "@types/compression": "^1.7.2",
    "@types/d3-scale": "*",
    "@types/file-saver": "*",
    "@types/gl-matrix": "*",
    "@types/lodash.flatten": "*",
    "@types/lodash.floor": "*",
    "@types/lodash.groupby": "*",
    "@types/lodash.random": "*",
    "@types/luxon": "*",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "*",
    "@types/regression": "^2.0.2",
    "@types/sat": "*",
    "@types/uuid": "*",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "caniuse-lite": "^1.0.30000697",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "express": "^4.18.2",
    "firebase-tools": "^12.8.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^8.0.2",
