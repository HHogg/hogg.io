{
  "name": "@hogg/circular-sequence",
  "version": "0.0.0",
  "author": "Harry Hogg <harry@hogg.io>",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "build": "wasm-pack build --target web",
    "clean": "rm -rf ./pkg",
    "dev": "cargo watch -w ./src-rust -w ../canvas/src-rust -q -s 'yarn workspace @hogg/circular-sequence run build'",
    "generate:types": "typeshare ./src-rust ../canvas/src-rust --lang=typescript --output-file=./src/types.ts"
  },
  "dependencies": {
    "@hogg/canvas": "workspace:^",
    "@hogg/common": "workspace:^",
    "preshape": "^17.2.0",
    "react": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "typescript": "^5.0.4"
  }
}
