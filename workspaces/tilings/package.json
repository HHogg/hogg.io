{
  "name": "@hogg/tilings",
  "version": "0.0.0",
  "author": "Harry Hogg <harry@hogg.io>",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "build": "yarn generate:types",
    "clean": "rm -rf ./logs",
    "export:results": "cargo run  --release --bin tiling-export",
    "generate:images": "node ./scripts/generate-images.js",
    "generate:results": "cargo run  --release --bin tiling-searcher -- --reset --log-to-file",
    "generate:types": "typeshare ./src-rust --lang=typescript --output-file=./src/types.ts"
  },
  "dependencies": {
    "@hogg/canvas": "workspace:^",
    "@hogg/common": "workspace:^",
    "@hogg/tiling-parser": "workspace:^",
    "@hogg/tiling-renderer": "workspace:^",
    "lucide-react": "^0.294.0",
    "preshape": "^17.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.8.0",
    "@swc/core": "^1.4.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "puppeteer": "^21.4.1",
    "sharp": "^0.32.6",
    "typescript": "^5.0.4"
  }
}
