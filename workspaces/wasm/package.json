{
  "name": "@hogg/wasm",
  "version": "0.0.0",
  "author": "Harry Hogg <harry@hogg.io>",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "build": "yarn build:types && yarn build:wasm",
    "build:types": "typeshare --lang=typescript --output-file=./src/types.ts  ../",
    "build:wasm": "wasm-pack build --target web",
    "clean": "rm -rf ./pkg && rm -rf ./src/types.ts",
    "dev": "cargo watch -w ./src-rust -w ../circular-sequence/src-rust -w ../line-segment-extending/src-rust -w ../spatial-grid-map/src-rust -w ../tilings/src-rust -q -s 'yarn workspace @hogg/wasm run build'"
  },
  "dependencies": {
    "@hogg/circular-sequence": "workspace:^",
    "@hogg/common": "workspace:^",
    "@hogg/line-segment-extending": "workspace:^",
    "lodash.get": "^4.4.2",
    "lucide-react": "0.445.0",
    "preshape": "^19.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11"
  }
}
